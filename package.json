{
  "name": "foo-agent",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "react-router dev --port 5273",
    "build": "react-router build",
    "preview": "vite preview --port 5273",
    "deploy": "react-router build && wrangler deploy",
    "deploy:staging": "CLOUDFLARE_ENV=staging pnpm run deploy",
    "deploy:production": "CLOUDFLARE_ENV=production pnpm run deploy",
    "kill-ports": "lsof -ti:5273 | xargs -r kill -9 && lsof -ti:9329 | xargs -r kill -9 && echo 'Killed processes on ports 5273 and 9329' || echo 'No processes found on ports 5273 and 9329'",
    "logs:deployed": "wrangler tail",
    "logs:staging": "wrangler tail --env staging",
    "test": "vitest",
    "types": "wrangler types",
    "format": "prettier --write .",
    "check": "prettier . --check && biome lint && tsc"
  },
  "keywords": [
    "cloudflare",
    "ai",
    "agents"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@cloudflare/puppeteer": "^1.0.3",
    "@cloudflare/vite-plugin": "1.13.10",
    "@cloudflare/vitest-pool-workers": "^0.8.63",
    "@cloudflare/workers-types": "^4.20250813.0",
    "@react-router/dev": "^7.8.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "htm": "^3.1.1",
    "prettier": "^3.6.2",
    "react-router": "^7.8.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "3.2.4",
    "wrangler": "^4.29.1"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/google": "^1.2.19",
    "@ai-sdk/openai": "^1.3.23",
    "@ai-sdk/react": "^1.2.12",
    "@ai-sdk/ui-utils": "^1.2.11",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-avatar": "^1.1.10",
