{
  "name": "foo-agent",
  "main": "src/server.ts",
  "compatibility_date": "2025-05-07",
  "compatibility_flags": [
    "nodejs_compat",
    "nodejs_compat_populate_process_env",
  ],
  "assets": {
    "directory": "public",
    "binding": "ASSETS",
    "not_found_handling": "none",
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "AppAgent",
        "class_name": "AppAgent",
      },
    ],
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["AppAgent"],
    },
  ],
  "vars": {
    "GATEWAY_BASE_URL": "https://gateway.atyourservice.ai",
    "OAUTH_PROVIDER_BASE_URL": "https://atyourservice.ai",
    "SETTINGS_ENVIRONMENT": "dev",
    "COMPOSIO_API_KEY": "",
    "BROWSERBASE_API_KEY": "",
    "ATYOURSERVICE_OAUTH_REDIRECT_URI": "http://localhost:5273/auth/callback",
  },
  "dev": {
    "port": 8273,
    "inspector_port": 9330,
  },
  "env": {
    "staging": {
      "durable_objects": {
        "bindings": [
          {
            "name": "AppAgent",
            "class_name": "AppAgent",
          },
        ],
      },
      "migrations": [
        {
          "tag": "v1",
          "new_sqlite_classes": ["AppAgent"],
        },
      ],
      "vars": {
        "GATEWAY_BASE_URL": "https://gateway.atyourservice.ai",
        "OAUTH_PROVIDER_BASE_URL": "https://atyourservice.ai",
        "SETTINGS_ENVIRONMENT": "staging",
        "COMPOSIO_API_KEY": "",
        "BROWSERBASE_API_KEY": "",
        "ATYOURSERVICE_OAUTH_REDIRECT_URI": "https://foo-agent-staging.atyourservice.ai/auth/callback",
      },
      "routes": [
        {
          "pattern": "foo-agent-staging.atyourservice.ai",
          "custom_domain": true,
        },
      ],
    },
    "production": {
      "durable_objects": {
        "bindings": [
          {
            "name": "AppAgent",
            "class_name": "AppAgent",
          },
        ],
      },
      "migrations": [
        {
          "tag": "v1",
          "new_sqlite_classes": ["AppAgent"],
        },
      ],
      "vars": {
        "GATEWAY_BASE_URL": "https://gateway.atyourservice.ai",
        "OAUTH_PROVIDER_BASE_URL": "https://atyourservice.ai",
        "SETTINGS_ENVIRONMENT": "production",
        "COMPOSIO_API_KEY": "",
        "BROWSERBASE_API_KEY": "",
        "ATYOURSERVICE_OAUTH_REDIRECT_URI": "https://foo-agent.atyourservice.ai/auth/callback",
      },
      "routes": [
        {
          "pattern": "foo-agent.atyourservice.ai",
          "custom_domain": true,
        },
      ],
    },
  },
}
